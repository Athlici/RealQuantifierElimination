              RowBox[{"b", "[", "#", "]"}]}], "&"}], "/@", 
            RowBox[{"Range", "[", "m", "]"}]}], ")"}]}], "&&", 
         RowBox[{"And", "@@", "\[IndentingNewLine]", 
          RowBox[{"Thread", "[", 
