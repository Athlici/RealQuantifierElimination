           RowBox[{
            RowBox[{
             RowBox[{
              RowBox[{"t", "[", "#", "]"}], "<", 
              RowBox[{"t", "[", 
               RowBox[{"#", "+", "1"}], "]"}]}], "&"}], "/@", 
            RowBox[{"Range", "[", 
             RowBox[{"n", "-", "1"}], "]"}]}], ")"}]}], "&&", 
