                 RowBox[{"{", 
                  RowBox[{"i", ",", "1", ",", "n"}], "}"}]}], "]"}], 
               RowBox[{"Product", "[", 
                RowBox[{
                 RowBox[{
                  RowBox[{"x", "^", "2"}], "+", 
                  RowBox[{"2", 
                   RowBox[{"a", "[", "i", "]"}], " ", "x"}], "+", 
                  RowBox[{"b", "[", "i", "]"}]}], ",", 
                 RowBox[{"{", 
                  RowBox[{"i", ",", "1", ",", "m"}], "}"}]}], "]"}]}], ",", 
              "x"}], "]"}], "\[Equal]", 
            RowBox[{"Join", "[", 
             RowBox[{
              RowBox[{"y", "/@", 
               RowBox[{"Range", "[", 
                RowBox[{"d", "-", "1"}], "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"0", ",", "1"}], "}"}]}], "]"}]}], "]"}]}]}]}], "]"}], 
      "&"}], "@", 
     RowBox[{"Join", "[", 
      RowBox[{
       RowBox[{"t", "/@", 
        RowBox[{"Range", "[", "n", "]"}]}], ",", 
       RowBox[{"a", "/@", 
        RowBox[{"Range", "[", "m", "]"}]}], ",", 
       RowBox[{"b", "/@", 
        RowBox[{"Range", "[", "m", "]"}]}]}], "]"}]}]}], "]"}]}]], "Input",
